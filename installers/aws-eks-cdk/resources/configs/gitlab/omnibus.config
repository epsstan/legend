external_url 'https://__GITLAB_EXTERNAL_URL__/'
gitlab_rails['lfs_enabled'] = true
nginx['listen_port'] = 443
nginx['ssl_certificate'] = '/etc/gitlab/ssl/self.crt'
nginx['ssl_certificate_key'] = '/etc/gitlab/ssl/self.key'
letsencrypt['enable'] = false
gitlab_rails['initial_root_password'] = '__GITLAB_ROOT_PASSWORD__'